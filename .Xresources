*foreground: #ffffff
*foreground_bold: #a8a19f
*cursor: #a8a19f
*background: #1b1918

! Black + DarkGrey
*color0: #1b1918
*color8: #686868

! DarkRed + Red
*color1: #ff5c57
*color9: #ff5c57

! DarkGreen + Green
*color2: #5af78e
*color10:#5af78e

! DarkYellow + Yellow
*color3: #f3f99d
*color11:#f3f99d

! DarkBlue + Blue
*color4: #57c7ff
*color12:#57c7ff

! DarkMagenta + Magenta
*color5: #ff6ac1
*color13:#ff6ac1

! DarkCyan + Cyan (both not tango)
*color6: #9aedfe
*color14:#9aedfe

! LightGrey + White
*color7: #eff0eb
*color15:#f1f1f0

URxvt.intensityStyles:	false
!!URxvt.background:       [70]#000000
URxvt.depth:		32
URxvt.font: 		xft:monospace:size=11,xft:Inconsolata for Powerline
*font: 			Inconsolata for Powerline
URxvt.letterSpace:	-1
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.perl-ext-common:	default,matcher,resize-font
URxvt.perl-lib:		$HOME/.config/Scripts
URxvt.url-launcher:	/usr/bin/xdg-open
URxvt.matcher.button:	1
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up

URxvt.colorUL:		#4682B4

! rofi
rofi.lines:		18
rofi.modi:		window,drun,run,ssh
rofi.font:		Monospace Regular 12
rofi.theme:		/usr/share/rofi/themes/Monokai.rasi
rofi.drun-icon-theme:	Paper
